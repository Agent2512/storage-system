{
  "name": "mywebtest",
  "module": "src/server.tsx",
  "type": "module",
  "devDependencies": {
    "bun-types": "latest",
    "typed-html": "^3.0.1"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@elysiajs/cookie": "^0.7.0",
    "@elysiajs/html": "^0.7.3",
    "@elysiajs/jwt": "^0.7.1",
    "@prisma/client": "5.6.0",
    "dayjs": "^1.11.10",
    "elysia": "^0.7.21",
    "prisma": "^5.6.0",
    "tailwindcss": "^3.3.5"
  },
  "scripts": {
    "dev": "bun run --watch src/server.tsx",
    "dev:hot": "bun run --hot src/server.tsx",
